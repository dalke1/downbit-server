<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.darc.downbit.dao.mapper.FileMapper">

    <resultMap id="BaseResultMap" type="com.darc.downbit.dao.entity.File">
        <id property="fileId" column="file_id" jdbcType="INTEGER"/>
        <result property="fileType" column="file_type" jdbcType="OTHER"/>
        <result property="fileName" column="file_name" jdbcType="VARCHAR"/>
        <result property="createdTime" column="created_time" jdbcType="TIMESTAMP"/>
        <result property="updatedTime" column="updated_time" jdbcType="DATE"/>
    </resultMap>

    <sql id="Base_Column_List">
        file_id,file_type,file_name,
        created_time,updated_time
    </sql>
</mapper>
